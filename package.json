{
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "dev": "pnpm -F ./web dev",
    "build": "pnpm -F ./web build",
    "build:wasm": "wasm-pack build lib --release --out-name=lib --scope tedbyron --target web",
    "preview": "pnpm -F ./web preview",
    "publish:wasm": "wasm-pack publish lib --access public",
    "lint": "pnpm -F ./web lint",
    "format": "pnpm -F ./web format",
    "test": "wasm-pack test --node lib"
  }
}
